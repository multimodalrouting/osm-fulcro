        "d3-shape": "^1.3.5",
        "highlight.js": "^9.13.1",
        "leaflet": "^1.5.1",
        "osmtogeojson": "^3.0.0-beta.3",
        "pbf": "^3.2.1",
        "react-grid-layout": "^0.16.6",
        "react-icons": "^2.2.7",
        "react-leaflet": "^2.4.0",
        "react-leaflet-control": "^2.1.1",
        "react-leaflet-d3": "^2.0.0",
        "react-leaflet-d3-svg-overlay": "^1.1.0",
        "react-leaflet-sidetabs": "^1.0.1",
        "react-leaflet-vectorgrid": "^2.2.1",
        "reakit": "^0.11.2",
        "semantic-ui-react": "^0.88.1"
